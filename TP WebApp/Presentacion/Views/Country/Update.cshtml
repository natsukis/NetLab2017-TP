@model Services.CountryModel

<form action="/country/update" method="post">
    <div>
        <label>Id</label>
        <input type="text" name="ID" value="@Model.ID" />
    </div>
    <div>
        <label>Country Name</label>
        <input type="text" name="CountryName" value="@Model.CountryName" />
    </div>
    <div>
        <button type="submit">Save</button>
    </div>
</form>