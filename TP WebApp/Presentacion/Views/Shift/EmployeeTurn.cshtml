@model List<Services.Models.ShiftControlModel>

<table>
    <thead>
        <tr>
            <th>Register</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in this.Model)
        {
            <tr>
                <td>@employee.ID - @employee.Employee.FirstName @employee.Employee.LastName - @employee.Entry - @employee.Exit - @employee.WorkedHours - <a href="/shift/shiftcontrol?shiftControlModel=@employee">AgregarHorario</a></td>
            </tr>
        }
    </tbody>
</table>